@using MyPortfolioAspNetMvc5.Models.Entity
@model List<Abouts>
<div class="container">
    @foreach (var item in Model)
    {
        <section class="resume-section" id="about">
            <div class="resume-section-content">
                <h1 class="mb-0">
                    @item.Name
                    <span class="text-primary">@item.Surname</span>
                </h1>
                <div class="subheading mb-5">
                    @item.Adress @item.PhoneNumber
                    <a href="mailto:@item.Mail">@item.Mail</a>
                </div>
                <p class="lead mb-5">@item.Description</p>

                @Html.Action("SocaialMediaPartial")
             
            </div>
        </section>
    }

</div>
